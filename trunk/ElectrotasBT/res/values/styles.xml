<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="navSection">
        <item name="android:textColor">@color/Blanco</item>
        <item name="android:textSize">22sp</item>
    </style>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->


    <!-- Application theme. -->
    <style name="AppTheme" parent="@style/Theme.AppCompat">
        <item name="android:windowBackground">@color/Blanco</item>
        <item name="actionBarStyle">@style/MyActionBar</item>
    </style>

    <style name="MyActionBar" parent="@style/Widget.AppCompat.ActionBar.Solid">
        <item name="background">@color/Celeste</item>
        <item name="android:textColor">@color/Blanco</item>
        <!-- <item name="displayOptions"></item> -->
    </style>

    <style name="txtImportante" parent="android:attr/textAppearanceMedium">
        <item name="android:textColor">@color/Negro</item>
        <item name="android:textSize">22sp</item>
    </style>
    
</resources>